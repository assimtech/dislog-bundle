{
    "name": "assimtech/dislog-bundle",
    "type": "library",
    "description": "Dislog symfony bundle",
    "homepage": "https://github.com/assimtech/dislog-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8",

        "assimtech/dislog": "^2.3",
        "assimtech/sysexits": "^1",
        "symfony/console": "^3|^4|^5",
        "symfony/http-kernel": "^3|^4|^5"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.7|^2",
        "phing/phing": "*",
        "phpmd/phpmd": "*",
        "psr/http-client": "^1",
        "psr/http-message": "^1",
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "suggest": {
        "guzzlehttp/psr7": "Required if using LoggingHttpClient",
        "psr/http-client": "Required if using LoggingHttpClient",
        "psr/http-message": "Required if using LoggingHttpClient"
    },
    "autoload": {
        "psr-4": {
            "Assimtech\\DislogBundle\\": "src"
        }
    }
}
